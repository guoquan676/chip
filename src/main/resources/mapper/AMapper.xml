<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.template.chip.repository.mapper.IAMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.template.chip.repository.po.APO">
        <id column="id" property="id" />
        <result column="company_name" property="companyName" />
        <result column="created_time" property="createdTime" />
        <result column="user_id" property="userId" />
        <result column="customer_id" property="customerId" />
        <result column="status" property="status" />
        <result column="jsonstr" property="jsonstr" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, company_name, created_time, user_id, customer_id, status, jsonstr
    </sql>

</mapper>
